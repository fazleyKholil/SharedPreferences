<?xml version="1.0" encoding="UTF-8"?>
    <plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android" id="cordova-plugin-ArkSharedPref" version="0.0.0">
        <name>ArkSharedPref</name>
        <description>Cordova Shared preference plugin</description>
        <license>Apache 2.0</license>
        <keywords>
            ArkSharedPref, ArkSharedPref, Preferences, storage, data, local storage
        </keywords>
        <repo></repo>
        <issue></issue>

        <js-module src="www/ArkSharedPref.js" name="ArkSharedPref">
            <clobbers target="ArkSharedPref" />
        </js-module>

        <!-- android -->
        <platform name="android">
            <config-file target="res/xml/config.xml" parent="/*">
                <feature name="ArkSharedPref">
                    <param name="android-package" value="ArkSharedPref" />
                </feature>
            </config-file>
            <source-file src="src/android/ArkSharedPref.java" target-dir="src/" />
        </platform>

    </plugin>
